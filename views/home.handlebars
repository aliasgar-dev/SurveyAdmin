<div class="main-container" id="homeUI">
   
    <div>
        {{# isloggedin}}
            <a href="/logout"> <button type="btn" class="logoutClss pull-right" title="logout"></button></a>
        {{/isloggedin}}
    </div>
  	<!-- <span id="createNewSurvey " class=""></span> -->
    <div class="surveyCreateCls" id="createNewSurvey">Create New Surveys</div>
    <div class="checkWalletCls">
      <a href="/analytics">Go To Analytics Page</a>
    </div>
  	<table class="table" style="margin-top: 20px;">
      <thead>
          <tr>
            	<th scope="col">Survey Name</th>
            	<th scope="col">Reward Amount</th>
            	<!-- <th scope="col">Policy Start Date</th>
            	<th scope="col">Policy End Date</th>
            	<th scope="col">Category Name</th>
            	<th scope="col">Company Name</th> -->
          </tr>
      </thead>
      <tbody id="allSurveyHolder">
    </tbody>
  </table>


    <div id="createNewSurveyModal" class="modal fade " role="dialog">
  		<div class="modal-dialog">
		    <div class="modal-content newSurveyModalCls">
		      	<div class="modal-header">
		        	<h4 class="modal-title">New Survey Form</h4>
		      	</div>
		      	<div class="modal-body newSurveyClass">
		        	<form id="newSurveyForm" class="newSurveyFormCls">
		        		<div class="form-group">
						    <label for="newSurveyName">Enter Survey Name</label>
						    <input type="text" class="form-control" id="newSurveyName" placeholder="Enter Survey Name" name="newSurveyName">
						</div>
						<div class="form-group">
						    <label for="newSurveyGiftAmount">Enter Gift Amount</label>
						    <input type="text" class="form-control" id="newSurveyGiftAmount" placeholder="Enter Amount" name="newSurveyGiftAmount">
						</div>
						<div id="moreSurveyFormHolder"></div>
					</form>
	      			<div id="addMoreSurveyForm">Add More Fields</div>

		      		
		        	<!-- <p>Some text in the modal.</p> -->
		      	</div>
		      	<div class="modal-footer">
		        	<span id="closeAddSurveyModal" class="btn-danger scheduleFormSaveCls">Close</span>
                    <span id="saveNewSurey" class="btn-info scheduleFormSaveCls">Save</span>
		      	</div>
		    </div>
  		</div>
	</div>
    
</div>